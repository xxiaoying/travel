ellipsis()
  overflow: hidden
  white-space: nowrap
  text-overflow: ellipsis
  
// 标签4 模块小标签
title()
  font-size: 0.32rem
  line-height: 0.88rem
  height: 0.88rem
  
ellipsis2()
  display: -webkit-box
  overflow: hidden
  text-overflow: ellipsis
  -webkit-line-clamp: 2
  -webkit-box-orient: vertical
iconBackCircle()
  position: absolute
  top: .1rem
  left: .1rem
  width: .72rem
  height: .72rem
  border-radius: 50%
  text-align: center
  background: rgba(0, 0, 0, .5)
  color: $colFff
  font-size: .32rem
  font-weight: bold
  display: flex
  flex-direction: column
  justify-content: center